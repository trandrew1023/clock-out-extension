{
  "manifest_version": 2,
  "name": "Clock Out Tool",
  "description": "Find out when you need to clock out",
  "version": "1.0",
  "icons": {
    "512": "clock.png"
  },
  "browser_action": {
    "default_icon": "clock.png",
    "default_popup": "clockout.html",
    "js": "clockout.js"
  },
  "permissions": ["activeTab"]
}
